{
    "name": "RGB Dark Theme",
    "version": "1.0.0",
    "description": "A dark theme with RGB color-changing effects.",
    "author": "FPSFIGHTER",
    "theme": {
        "backgroundColor": "#1e1e1e",
        "textColor": "rgb(255, 255, 255)",
        "rgbElements": [
            {
                "selector": ".title",
                "classes": ["rgb-text"]
            },
            {
                "selector": ".username",
                "classes": ["rgb-text"]
            }
        ]
    }
}
